[sub_resource type="Shader" id=3]
code = "shader_type canvas_item;

uniform float cutoff = 0.2;
uniform float gradient_scale = 8.0;

uniform vec2 target = vec2(0.5);

void fragment()
{
	vec2 uv = vec2(SCREEN_UV.x * (SCREEN_PIXEL_SIZE.y / SCREEN_PIXEL_SIZE.x) - 0.36, SCREEN_UV.y);
	float dist = length(uv - vec2(target.x, 1.0 - target.y));
	float t = clamp((dist - cutoff) * gradient_scale, 0.0, 1.0);
	COLOR = texture(TEXTURE, UV) * t;
}"

